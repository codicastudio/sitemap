{
    "name": "codicastudio/sitemap",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/sitemap",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0",
        "nesbot/carbon": "^1.21|^2.0",
        "spatie/crawler": "^4.1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-snapshot-assertions": "^3.0.0",
        "codicastudio/temporary-directory": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Sitemap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Sitemap\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
